{
    "name": "yii2mod/yii2-selectize",
    "description": "selectize.js wrapper for yii2",
    "type": "yii2-extension",
    "keywords": [
        "yii2 selectize",
        "yii2 selectize.js",
        "yii2 select",
        "yii2 tagging"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoy",
            "email": "igorzfort@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-jui": "~2.0.0",
        "bower-asset/selectize.js": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "yii2mod\\selectize\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
